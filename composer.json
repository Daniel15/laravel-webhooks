{
    "name": "oz/webhook",
    "description": "Easily setup and organize your webhooks.",
    "type": "library",
    "require": {
        "laravel/framework": "^5.3",
        "mockery/mockery": "^0.9.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Osvaldo Brignoni",
            "email": "hello@obrignoni.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Oz\\Webhook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oz\\Webhook\\Test\\": "test",
            "Oz\\Webhook\\Stub\\": "stub"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
